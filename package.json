{
  "name": "@arcadecity/app",
  "description": "The Bitcoin gig economy",
  "version": "6.0.0",
  "repository": "git@github.com:ArcadeCity/arcade.git",
  "author": "Christopher David <chris@arcade.city>",
  "license": "CC0-1.0",
  "main": "index.js",
  "scripts": {
    "start": "rm -rf dist && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "postinstall": "npx patch-package",
    "test": "jest --watchAll",
    "build:android": "eas build -p android",
    "build:android:apk": "eas build -p android --profile apk",
    "build:ios": "eas build -p ios",
    "build:ios:local": "eas build --platform ios --local",
    "submit:android": "eas submit -p android",
    "submit:ios": "eas submit -p ios",
    "publish:ios": "eas update --branch production -p ios --message 'Update'",
    "publish:android": "eas update --branch production -p android --message 'Update'"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@alephium/noble-secp256k1": "1.5.5",
    "@expo-google-fonts/inter": "^0.2.2",
    "@expo-google-fonts/lexend": "^0.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@react-spring/native": "^9.5.2",
    "@scure/bip32": "1.1.0",
    "@scure/bip39": "1.1.0",
    "@shopify/react-native-skia": "0.1.141",
    "bech32": "^2.0.0",
    "big-integer": "^1.6.51",
    "create-hash": "1.2.0",
    "expo": "~46.0.7",
    "expo-asset": "~8.6.1",
    "expo-clipboard": "~3.1.0",
    "expo-constants": "~13.2.3",
    "expo-font": "~10.2.0",
    "expo-linking": "~3.2.2",
    "expo-localization": "~13.1.0",
    "expo-random": "~12.3.0",
    "expo-secure-store": "~11.3.0",
    "expo-splash-screen": "~0.16.1",
    "expo-status-bar": "~1.4.0",
    "expo-system-ui": "~1.3.0",
    "expo-updates": "~0.14.4",
    "expo-web-browser": "~11.0.0",
    "i18n-js": "3.9.2",
    "mobx": "^6.6.1",
    "mobx-react-lite": "^3.4.0",
    "mobx-state-tree": "^5.1.6",
    "moment": "^2.29.4",
    "moti": "0.18.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.4",
    "react-native-reanimated": "~2.9.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-web": "~0.18.7",
    "react-native-webview": "11.23.0",
    "reactotron-mst": "^3.1.4",
    "reactotron-react-native": "^5.0.2",
    "text-encoding-polyfill": "0.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "jest": "^26.6.3",
    "jest-expo": "~44.0.1",
    "node-libs-react-native": "1.2.1",
    "react-test-renderer": "18.0.0",
    "typescript": "~4.3.5"
  },
  "private": true,
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "resolutions": {
    "@noble/hashes": "1.1.2"
  }
}
